{
  "language": {
    "name": "Rust",
    "version_constraint": ">=1.77"
  },
  "package_manager": {
    "name": "cargo",
    "install_command": "cargo install",
    "config_file": "Cargo.toml"
  },
  "build_tools": [
    "cargo"
  ],
  "test_framework": [
